# Example .env for tic-tac-toe
# Copy this to a file named .env and customize values for your environment

# Secret key used by Flask sessions
SECRET_KEY=tic-tac-toe-login-secret

# Host the app will bind to
HOST=0.0.0.0

# Port the app will bind to (integer)
FLASK_RUN_PORT=5050

# Debug mode: True/False
DEBUG=True

# Optional: comma-separated list or '*' for allowed origins (not automatically wired in code)
CORS_ALLOWED_ORIGINS=*
